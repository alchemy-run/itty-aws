{
  "name": "distilled-aws-sdk",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "setup": "bun run scripts/setup.ts",
    "generate": "bun run scripts/generate-clients.ts",
    "build": "bun generate && tsc -b"
  },
  "files": [
    "src",
    "lib"
  ],
  "exports": {
    "./*": {
      "bun": "./src/services/*.ts",
      "types": "./lib/services/*.d.ts",
      "default": "./lib/services/*.js"
    },
    ".": {
      "bun": "./src/index.ts",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "dependencies": {
    "@aws-sdk/credential-providers": "^3.933.0",
    "@effect/platform": "latest",
    "@effect/platform-bun": "latest",
    "aws4fetch": "^1.0.20",
    "effect": "latest",
    "fast-xml-parser": "^5.3.2"
  },
  "devDependencies": {
    "@effect/cluster": "latest",
    "@effect/experimental": "latest",
    "@effect/rpc": "latest",
    "@effect/sql": "latest",
    "@effect/workflow": "latest",
    "@rsbuild/core": "^1.6.7",
    "@types/bun": "latest",
    "@types/node": "^24.10.1",
    "@typescript/native-preview": "^7.0.0-dev.20251230.1",
    "dedent": "^1.7.0",
    "typescript": "^5"
  }
}
