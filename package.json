{
  "name": "itty-aws",
  "version": "0.0.9",
  "files": [
    "lib"
  ],
  "type": "module",
  "main": "./lib/src/index.js",
  "module": "./lib/src/index.js",
  "types": "./lib/src/index.d.ts",
  "scripts": {
    "download:models": "bash ./scripts/download-smithy-models.sh",
    "generate": "pnpm generate:types && pnpm generate:mappings",
    "generate:types": "ts-node-esm ./scripts/gen-sdk-types.mts",
    "generate:mappings": "ts-node-esm ./scripts/gen-sdk-mappings.mts",
    "build": "rm -rf ./lib && tsc --build tsconfig.build.json",
    "test": "ts-node-esm --project ./tsconfig.esm.json ./test/setup.ts && NODE_OPTIONS=--experimental-vm-modules jest",
    "watch": "tsc --build --watch tsconfig.build.json",
    "analyze:bundle": "sh ./scripts/analyze-bundle.sh",
    "bench:bootstrap": "pnpm -r --filter @itty-aws/benchmark bootstrap",
    "bench:synth": "pnpm -r --filter @itty-aws/benchmark synth",
    "bench:deploy": "pnpm -r --filter @itty-aws/benchmark run deploy",
    "bench:hotswap": "pnpm -r --filter @itty-aws/benchmark run hotswap",
    "bench:run": "pnpm -r --filter @itty-aws/benchmark start",
    "bench:analyse": "pnpm -r --filter @itty-aws/benchmark analyse"
  },
  "dependencies": {
    "@rgrove/parse-xml": "^4.0.1",
    "aws-sdk": "2.1295.0"
  },
  "peerDependencies": {
    "@aws-crypto/sha256-js": "^3",
    "@aws-sdk/credential-providers": "^3",
    "@aws-sdk/protocol-http": "^3",
    "@aws-sdk/signature-v4": "^3",
    "@aws-sdk/types": "^3"
  },
  "devDependencies": {
    "@aws-crypto/sha256-js": "3.0.0",
    "@aws-sdk/credential-provider-env": "3.226.0",
    "@aws-sdk/protocol-http": "3.226.0",
    "@aws-sdk/signature-v4": "3.226.0",
    "@aws-sdk/types": "3.226.0",
    "@tsconfig/node16": "^1",
    "@types/jest": "^29",
    "@types/node": "^16.0.0",
    "@types/prettier": "^2.7.2",
    "esbuild": "0.17.2",
    "esbuild-visualizer": "^0.4.0",
    "jest": "^29",
    "node-fetch": "^3.3.0",
    "prettier": "^2.8.3",
    "standard-version": "^9.5.0",
    "ts-jest": "^29",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.test.json",
          "useESM": true
        }
      ]
    }
  }
}
